Package: steamr
Title: Steam Game Data Analysis
Version: 0.1.0
Authors@R: c(
    person("Maximilian", "KÃ¼hn", , "3009677@stud.hs-mannheim.de", role = c("aut", "cre")),
    person("Cedric", "Mingham", , "2021650@stud.hs-mannheim.de", role = c("aut")),
    person("Katharina", "Salewski", , "1911459@stud.hs-mannheim.de", role = c("aut"))
